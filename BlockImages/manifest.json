{
"manifest_version": 2,
    "name": "EZ Load",
	
	
    "description": "EZ Load",
    "version": "1.1",
   
    
    "background": {
        "scripts": [
            "blockpics.js"
			
        ]
    },
    
    "icons": { "1482": "2.png" },
      
      
    "browser_action": {
        "default_icon": {
            "19": "images/icon19-disabled.png"
			
        },
		
        "default_title": "PAD_SEF05"
    },
	
	

    "permissions": [
		"tabs",
		"http://*/*",
		"https://*/*",
		"contentSettings", "tabs", "https://ajax.googleapis.com/",
		"contextMenus",
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"
    ]
}